set key default
set key box
set key ins vert
set key left top

set terminal png size 600, 400
set output 'p1q3.png'

set title "λ versus x_{k}^{*}" font "Helvetica,20" norotate
set xlabel "λ" font "Helvetica,15"
set ylabel "x_{k}^{*}" font "Helvetica, 15" norotate
set style line 1 lc rgb "blue"

set xrange [0.5:4]

plot '../data/questao2.dat' using 2:1 with lines title 'λ = [0.4, 2.9]', \
    for [col=2:1001] '../data/questao3.dat' using 1:(column(col) == 0 ? NaN : column(col)) with lines ls 1 notitle
