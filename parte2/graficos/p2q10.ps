set key default
set key box
set key ins vert
set key right top

set terminal png size 600, 400
set output 'p2q10.png'

set title "T_{i} versus t(T_{i})" font "Helvetica,20" norotate
set xlabel "T_{i}" font "Helvetica,15"
set ylabel "t(T_{i})" font "Helvetica, 15"
set style line 1 lc rgb "blue"

plot '../data/questao10.dat' using 1:2 with lines ls 1 title 'Variação de t com T_{i}'
