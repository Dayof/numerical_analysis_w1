set key default
set key box
set key ins vert
set key right top

set terminal png size 600, 400
set output 'p2q92.png'

set title "α versus t(α)" font "Helvetica,20" norotate
set xlabel "α" font "Helvetica,15"
set ylabel "t(α)" font "Helvetica, 15"
set style line 1 lc rgb '#0060ad' lt 1 lw 2 pt 7 pi -1 ps 1.5
